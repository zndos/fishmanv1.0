<%= turbo_frame_tag "new_catch" do %>
  <%= simple_form_for @catch,
                      remote: true,
                      data: { controller: "reset_form", action: "turbo:submit-end->reset_form#reset"} do |f| %>
    <%= f.input :name %>
    <%= f.input :weight %>
    <%= f.button :submit%>
  <% end %>
<% end %>

